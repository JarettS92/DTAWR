            data: {
              labels: ['Goal 1', 'Goal 2', 'Goal 3', 'Goal 4', 'Goal 5'],
              datasets: [{
                label: '% Complete',
                data: [30, 10, 100, 60, 70],
                backgroundColor: 'rgba(111, 45, 168, 0.5)'
              }]
            },
            options: {
              "legend":false, 
              "scale": {
                ticks: {
                  suggestedMin: 0,
                  suggestedMas: 100
                }
              }
            }
          });
        </script>
    </div><!-- End of Engagement Goals -->
